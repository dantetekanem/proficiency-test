.form-group
  = f.label :name
  = f.text_field :name, required: true, class: 'form-control'
.form-group
  = f.label :description
  = f.text_field :description, class: 'form-control'
.form-group
  = f.label 'Students'
  = f.select :student_ids, @students.collect { |c| [c.name, c.id] }, { include_blank: false }, { multiple: true, class: 'form-control' }
.form-group
  = f.label 'Status'
  = f.select :status, student_status, { include_blank: false }, { class: 'form-control' }
